<%= link_to list_path(list), class: 'list-item-link text-decoration-none' do %>
<div class="card-category" style="width: 22rem; background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, crop: :fill %>')">
  <%= list.name %>
</div>
<% end %>
